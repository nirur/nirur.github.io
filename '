            <nav id="secondary">
                <span>Jump to section</span>
                <script>
                    function generateJumpNav() {
                        const thisNav = document.getElementById("secondary");
                        var i = 1;
                        for (var sec of document.getElementsByTagName("h2")) {
                            var setid = "sect"+i;
                            sec.setAttribute("id", setid);
                            
                            var link = document.createElement("a");
                            var span = document.createElement("span");
                            span.setAttribute("class", "lbarbutton");
                            link.setAttribute("href", "#"+setid);
                            span.textContent = sec.textContent;
                            
                            link.appendChild(span);
                            thisNav.appendChild(link);
                            i++;
                        }
                        if (i>1) {
                            thisNav.setAttribute("style", "display: flex");
                        }
                    }
                    
                    document.body.onload = generateJumpNav;
                </script>
            </nav>
            <div id="scrollbox">
